// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Unicode_Emoji_Version {
  id            Int             @id @default(autoincrement())
  tag           String          @unique
  Unicode_Emoji Unicode_Emoji[]
}

model Unicode_Emoji {
  id                     Int                    @id @default(autoincrement())
  emoji                  String
  name                   String?
  known_as               String?
  description            String?
  codepoint              String?
  shortCode              String?
  Unicode_Emoji_Version  Unicode_Emoji_Version? @relation(fields: [versionId], references: [id])
  versionId              Int?
  proposal               String?
  isSupportingByChromium Boolean?               @default(false)
  testedChromiumVersion  String?
  emojipediaPage         String?
  keywords               String?
}
